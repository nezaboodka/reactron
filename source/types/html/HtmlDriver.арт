{
    операция create(block: Block<T, Нечто, C, Ничего>, b: объект { native: T?; controller: C? })
    операция initialize(block: Block<T, Нечто, C, Ничего>): Ничего
    операция finalize(block: Block<T, Нечто, C>, isLeader: Булево): Булево
    операция mount(block: Block<T, Нечто, C>): Ничего
    операция relocate(block: Block<T, Нечто, C>): Ничего
    операция render(block: Block<T, Нечто, C>): Ничего //| Promise<Ничего>
    общее blinkingEffectMarker: Текст?
    общая операция findEffectiveHtmlBlockHost(block: Block<Нечто>): Block<HTMLElement /* | SVGElement */>
    общая операция findPrevSiblingHtmlBlock(ties: Item<Block<Нечто>>): Item<Block<HTMLElement /* | SVGElement */>>?
}

